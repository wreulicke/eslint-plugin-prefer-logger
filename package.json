{
  "name": "eslint-plugin-prefer-logger",
  "version": "0.0.3",
  "description": "eslint plugin to prefer to use logger.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "test": "jest",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "README.md",
    "package.json",
    "lib"
  ],
  "keywords": [
    "eslint",
    "typescript",
    "example"
  ],
  "author": "wreulicke",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^6.1.3",
    "@types/node": "^13.1.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": "^6.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(src/.*\\.test)\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.ts$",
      "lib/.*"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
